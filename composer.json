{
    "name": "locr-company/splay-tree",
    "description": "Fast splay-tree data structure",
    "license": "MIT",
    "keywords": [
        "splay-tree"
    ],
    "authors": [
        {
            "name": "Ringo Leese",
            "email": "r.leese@locr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs ./src",
        "phpstan": "@phpstan-8.1",
        "phpstan-8.1": "./vendor/bin/phpstan --configuration=phpstan-8.1.neon",
        "phpstan-8.2": "./vendor/bin/phpstan --configuration=phpstan-8.2.neon",
        "phpstan-8.3": "./vendor/bin/phpstan --configuration=phpstan-8.3.neon",
        "test": "./vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Locr\\Lib\\": "src/"
        }
    }
}
